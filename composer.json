{
    "name": "orchestra/lumen",
    "description": "Lumen Framework for Orchestra Platform",
    "keywords": ["framework", "laravel", "lumen", "orchestral", "orchestra-platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Lumen\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "~5.2.7",
        "orchestra/foundation": "~3.2.0",
        "nikic/fast-route": "0.7.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.1",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "suggest": {
        "dingo/api": "Better API routing support (~1.0)",
        "tymon/jwt-auth": "Allows to use JWT-Auth support (~0.6)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "minimum-stability": "dev"
}
