{
    "name": "orchestra/lumen",
    "description": "Lumen Framework for Orchestra Platform",
    "keywords": ["framework", "laravel", "lumen", "orchestral", "orchestra-platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Lumen\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "nikic/fast-route": "0.4.*",
        "symfony/security-core": "2.7.*",
        "orchestra/config": "~3.1.0",
        "orchestra/auth": "~3.1.0",
        "orchestra/support": "~3.1.0",
        "orchestra/view": "~3.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.1",
        "dingo/api": "~1.0"
    },
    "suggest": {
        "dingo/api": "Better API routing support (~1.0)",
        "vlucas/phpdotenv": "Required to use .env files (~1.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "minimum-stability": "dev"
}
