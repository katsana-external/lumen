{
    "name": "orchestra/lumen",
    "description": "Lumen Framework for Orchestra Platform",
    "keywords": ["framework", "laravel", "lumen", "orchestral", "orchestra-platform"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/orchestral/lumen/issues",
        "source": "https://github.com/orchestral/lumen"
    },
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Lumen\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "~5.8.0",
        "laravie/api": "^2.1",
        "nikic/fast-route": "^1.3",
        "orchestra/foundation": "~3.8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "suggest": {
        "laravel/tinker": "Required to use the tinker console command (^1.0).",
        "laravie/api": "Better API routing support based on Dingo API (^2.1)",
        "tymon/jwt-auth": "Allows to use JWT-Auth support (^1.0)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.8-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
