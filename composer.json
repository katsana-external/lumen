{
    "name": "orchestra/lumen",
    "description": "Lumen Framework for Orchestra Platform",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "orchestral",
        "orchestra-platform"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/orchestral/lumen/issues",
        "source": "https://github.com/orchestral/lumen"
    },
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Lumen\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^8.0",
        "laravie/api": "5.x-dev",
        "nikic/fast-route": "^1.3",
        "orchestra/foundation": "7.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "suggest": {
        "laravel/tinker": "Required to use the tinker console command (^2.0).",
        "nyholm/psr7": "Required to use PSR-7 bridging features (^1.2).",
        "symfony/psr-http-message-bridge": "Required to use PSR-7 bridging features (^2.0).",
        "tymon/jwt-auth": "Allows to use JWT-Auth support (^1.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/foundation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/asset"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/auth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/contracts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/extension"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/html"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/kernel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/model"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/translation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/widget"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/memory"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/support"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/dhosa"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/query-filter"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/blueprint"
        }
    ]
}
